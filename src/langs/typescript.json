{
  "name":"typescript",
  "statmentTemplates":[
    {
      "id":"block",
      "requirements":[
        {
          "type":"token",
          "tokenType":"bracket",
          "tokenData":"{"
        },
        {
          "type":"statement",
          "repeat":-1
        },
        {
          "type":"token",
          "tokenType":"bracket",
          "tokenData":"}"
        }
      ]
    },
    {
      "id":"for-base",
      "abstract":true,
      "requirements":[
        {
          "type":"token",
          "tokenType":"keyword",
          "tokenData":"for"
        },
        {
          "type":"token",
          "tokenType":"parenthesis",
          "tokenData":"("
        },
        {
          "type":"statement",
          "repeat": 3
        },
        {
          "type":"token",
          "tokenType":"parenthesis",
          "tokenData":")"
        }
      ]
    },
    {
      "id":"for-block",
      "requirements":[
        {
          "type":"statement",
          "statementId":"for-base"
        },
        {
          "type":"statement",
          "statementId":"block"
        }
      ]
    },
    {
      "id":"for-inline",
      "requirements":[
        {
          "type":"statement",
          "statementId":"for-base"
        },
        {
          "type":"statement"
        }
      ]
    }
  ]
}